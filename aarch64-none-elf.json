{
    "abi-blacklist": [
      "stdcall",
      "fastcall",
      "vectorcall",
      "thiscall",
      "win64",
      "sysv64"
    ],
    "arch": "aarch64",
    "data-layout": "e-m:e-i8:8:32-i16:16:32-i64:64-i128:128-n32:64-S128",
    "executables": true,
    "linker": "aarch64-none-elf-gcc",
    "linker-flavor": "gcc",
    "linker-is-gnu": true,
    "dynamic-linking" : false, 
    "llvm-target": "aarch64-unknown-none",
    "no-compiler-rt": true,
    "features": "+a53,+strict-align",
    "max-atomic-width": 128,
    "os": "horizon",
    "panic": "abort",
    "panic-strategy": "abort",
    "position-independent-executables": true,
    "relocation-model": "pic",
    "target-c-int-width": "32",
    "target-endian": "little",
    "target-pointer-width": "64",
    "disable-redzone": true,
    "target-family" : "unix",
    "family" : "unix",
    "env" : "newlib",
    "target-env" : "newlib",
    "pre-link-args" : {
        "gcc" : [
            "-specs=/opt/devkitpro/libnx/switch.specs",
            "-march=armv8-a",
            "-mtune=cortex-a57", 
            "-mtp=soft", 
            "-fPIE",
            "-ffunction-sections",
            "-fno-rtti", 
            "-fno-exceptions",
            "-std=gnu++11",
            "-L/opt/devkitpro/devkitA64/lib/gcc/aarch64-none-elf/8.2.0",
            "-L/opt/devkitpro/devkitA64/aarch64-none-elf/lib",
            "-L/opt/devkitpro/libnx/lib"
        ]
    },
    "post-link-args" : {
        "gcc" : [
            "-lm",
            "-lnx",
            "-lc",
            "-lgcc",
            "-lc",
            "-lsysbase",
            "-lc",
            "-lgcc",
            "-lc",
            "-nodefaultlibs",
            "-Bstatic",
            "-pie",
            "-static-pie",
            "-Wl,-z,text"
        ]
    }
  }